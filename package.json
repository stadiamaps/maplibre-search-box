{
  "name": "@stadiamaps/maplibre-search-box",
  "version": "3.1.1",
  "homepage": "https://docs.stadiamaps.com/",
  "repository": "https://github.com/stadiamaps/maplibre-search-box",
  "license": "BSD-3-Clause",
  "description": "A MapLibre GL plugin to provide search and autocomplete using the Stadia Maps Search Autocomplete API.",
  "keywords": [
    "stadia maps",
    "maplibre-gl",
    "geocoding",
    "search",
    "autocomplete"
  ],
  "main": "./dist/maplibre-search-box.umd.js",
  "module": "./dist/maplibre-search-box.mjs",
  "style": "dist/maplibre-search-box.css",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/maplibre-search-box.umd.js",
      "import": "./dist/maplibre-search-box.mjs"
    },
    "./dist/*.css": {
      "require": "./dist/*.css",
      "import": "./dist/*.css"
    }
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint:scripts": "eslint .",
    "lint:styles": "stylelint ./**/*.{css,scss}",
    "format:scripts": "prettier . --write",
    "format:styles": "stylelint ./**/*.{css,scss} --fix",
    "format": "npm run format:scripts && npm run format:styles",
    "prepare": "husky",
    "uninstall-husky": "npm uninstall husky --no-save && git config --unset core.hooksPath && npx rimraf .husky"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "@vitest/browser": "^3.1.1",
    "@vitest/coverage-v8": "^3.1.1",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.5",
    "lint-staged": "^16.1.2",
    "postcss": "^8.4.21",
    "postcss-scss": "^4.0.6",
    "prettier": "^3.3.3",
    "sass": "^1.63.3",
    "stylelint": "^16.9.0",
    "stylelint-config-recommended": "^16.0.0",
    "stylelint-config-sass-guidelines": "^12.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.2.1",
    "vitest": "^3.1.1"
  },
  "peerDependencies": {
    "maplibre-gl": ">=2.0.0"
  },
  "dependencies": {
    "@stadiamaps/api": "^8.0.1"
  }
}
